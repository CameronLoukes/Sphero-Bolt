async function startProgram() {
	await scrollMatrixText('Start', { r: 66, g: 56, b: 255 }, 10, true)
	await roll(360, 90, 1.6)
	await setMainLed({ r: 0, g: 0, b: 255 })
	await delay(2)
	await roll(90, 90, 1)
	await delay(2)
	await Sound.Animal.Monkey.play() 
	await roll(360, 90, 0.7)
	await delay(2)
	await roll(210, 90, 0.9)
	await setMainLed({ r: 255, g: 0, b: 0 })
}



